<%= @course_date.inspect %>

<%= form_for [@course, @course_date] do |f| %>
	<%= f.label :start_date %>
	<%= f.datetime_select :start_date %>	
	<br>
	<%= f.label :end_date %>
	<%= f.datetime_select :end_date %>	
	<br>
	<%= f.label :quantity %>
	<%= f.text_field :quantity %>
	<br>
	<%= f.hidden_field :course_id %>		
	<%= f.submit %>
<% end %>
