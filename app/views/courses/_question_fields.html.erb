<fieldset>
  <%= f.text_field :start_date, :placeholder => "11/02/2015"%>
  <%= f.time_select :start_time, :class => "time-select" %>
  <%= f.text_field :end_date, :placeholder => "11/02/2015" %>
  <%= f.time_select :end_time %>
  <%= f.text_field :quantity, :placeholder => "20" %>
  <%= f.fields_for :tickets do |builder| %>
    <%= render 'answer_fields', f: builder %>
  <% end %>
  <%# link_to_add_fields "Add Answer", f, :tickets %>
</fieldset>