<%= form_for @booking, :multipart => true do |form| %>
	<%= form.label :name %>
	<%= form.text_field :name %>
	<br>
	<%= form.label :contact_number %>
	<%= form.text_field :contact_number %>
	<br>
	<%= form.label :email %>
	<%= form.text_field :email %>
	<br>
	<%= form.label :special_request %>
	<%= form.text_field :special_request %>
	<br>
	<%= form.fields_for :booking_dates do |test| %>
		<%= test.label :course_date_id %>
		<%= test.text_field :course_date_id %>
	<% end %>

	<%= form.submit %>
<% end %>